#!/bin/sh
# Description: Install vanilla-installer from source

wget https://github.com/Vanilla-OS/Albius/releases/download/continuous/albius.deb
apt-get install -y ./albius.deb
rm albius.deb

wget https://github.com/roxyboxxy/puppygirl-installer/releases/download/continuous/puppygirl-installer.deb
apt-get install -y ./puppygirl-installer.deb
rm puppygirl-installer.deb
